{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "cleanUrls": true,
    "redirects": [
      {
        "source": "/",
        "has": [
          { "type": "host", "value": "wiki.omkaarshenoy.com" }
        ],
        "destination": "/wiki/index.html",
        "permanent": false
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "has": [
          { "type": "host", "value": "wiki.omkaarshenoy.com" }
        ],
        "destination": "/wiki/$1"
      }
    ]
  }
  