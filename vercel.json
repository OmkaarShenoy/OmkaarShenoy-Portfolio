{
    "rewrites": [
      {
        "source": "/",
        "has": [
          { "type": "host", "value": "wiki.omkaarshenoy.com" }
        ],
        "destination": "/wiki/wiki.html"
      },
      {
        "source": "/(.*)",
        "has": [
          { "type": "host", "value": "wiki.omkaarshenoy.com" }
        ],
        "destination": "/wiki/$1"
      }
    ]
  }
  